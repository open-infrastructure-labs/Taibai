<?xml version="1.0"?>
<FieldsSettings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Settings>
        <fieldSetting>
            <fieldCnName>ID</fieldCnName><!-- excel的标题 -->
            <fieldDbName>id</fieldDbName><!-- 对象对应的属性名 -->
            <fieldWidth>10</fieldWidth><!-- 单元格宽度1-200 （不建议写200，不写默认自适应） -->
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>资源名称</fieldCnName>
            <fieldDbName>appName</fieldDbName>
            <fieldWidth>25</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>实例名称</fieldCnName>
            <fieldDbName>cmpInstanceName</fieldDbName>
            <fieldWidth>25</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>计量项</fieldCnName>
            <fieldDbName>meterageName</fieldDbName>
            <fieldWidth>25</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>计费项</fieldCnName>
            <fieldDbName>chargeName</fieldDbName>
            <fieldWidth>50</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>订单编号</fieldCnName>
            <fieldDbName>orderNo</fieldDbName>
            <fieldWidth>15</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>账单周期</fieldCnName>
            <fieldDbName>billCycleTimeStr</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>开始使用时间</fieldCnName>
            <fieldDbName>beginUseTimeStr</fieldDbName>
            <fieldWidth>25</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>结束使用时间</fieldCnName>
            <fieldDbName>finishUseTimeStr</fieldDbName>
            <fieldWidth>25</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>用量</fieldCnName>
            <fieldDbName>chargeUsage</fieldDbName>
            <fieldWidth>10</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>时长</fieldCnName>
            <fieldDbName>duration</fieldDbName>
            <fieldWidth>10</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>计费单位</fieldCnName>
            <fieldDbName>chargeUnit</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>单价（元）</fieldCnName>
            <fieldDbName>priceStr</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>折扣（%）</fieldCnName>
            <fieldDbName>discountStr</fieldDbName>
            <fieldWidth>10</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>总费用（元）</fieldCnName>
            <fieldDbName>totalChargeStr</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>说明</fieldCnName>
            <fieldDbName>remark</fieldDbName>
            <fieldWidth>100</fieldWidth>
        </fieldSetting>
    </Settings>
</FieldsSettings>