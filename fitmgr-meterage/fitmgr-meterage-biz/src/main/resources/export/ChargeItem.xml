<?xml version="1.0"?>
<FieldsSettings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Settings>
        <fieldSetting>
            <fieldCnName>主键ID</fieldCnName> <!-- excel的标题 -->
            <fieldDbName>id</fieldDbName> <!-- 对象对应的属性名 -->
            <fieldWidth>40</fieldWidth> <!-- 单元格宽度1-200 （不建议写200，不写默认自适应） -->
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>计费项名称</fieldCnName>
            <fieldDbName>chargeName</fieldDbName>
            <fieldWidth>50</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>计量项名称</fieldCnName>
            <fieldDbName>meterageItemName</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>云平台/组件Code</fieldCnName>
            <fieldDbName>cloudComponentCode</fieldDbName>
            <fieldWidth>50</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>计费属性</fieldCnName>
            <fieldDbName>chargeItemPropertyStr</fieldDbName>
            <fieldWidth>100</fieldWidth>
        </fieldSetting>
        <fieldSetting>
            <fieldCnName>单价（元）</fieldCnName>
            <fieldDbName>price</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>
        <!--<fieldSetting>
            <fieldCnName>计费项状态</fieldCnName>
            <fieldDbName>chargeStatusName</fieldDbName>
            <fieldWidth>20</fieldWidth>
        </fieldSetting>-->
        <fieldSetting>
            <fieldCnName>生效时间</fieldCnName>
            <fieldDbName>createTimeStr</fieldDbName>
            <fieldWidth>25</fieldWidth>
        </fieldSetting>
    </Settings>
</FieldsSettings>